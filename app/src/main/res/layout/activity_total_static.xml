<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context=".Reports.Activitys.TotalStaticActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:weightSum="360">

        <TextView
            android:id="@+id/txt_total_static_back"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="40"
            android:drawableLeft="@drawable/back_btn_image"
            android:fontFamily="@font/notosanscjkkr_bold"
            android:gravity="left|center_vertical"
            android:textColor="#343434"
            android:textSize="16dp"
            android:paddingLeft="12dp"/>

        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="280"
            android:text="종합보고서"
            android:textSize="18dp"
            android:gravity="center"
            android:textColor="@color/color343434"
            android:fontFamily="@font/notosanscjkkr_bold"
            android:includeFontPadding="false"/>

        <ImageView
            android:id="@+id/img_total_statics_shared"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="24"
            android:src="@drawable/ic_shared"/>
    </LinearLayout>

    <WebView
        android:id="@+id/webView_total_statics"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>